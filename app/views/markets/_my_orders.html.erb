<div id="my_orders_wrapper" class="has-left-dropdown">
  <div id="my_orders" class="panel panel-default">
    <div class="dropdown-wrapper">
      <ul class="dropdown-menu">
        <li>
          <%= link_to t('.clear_bids_html'), "/markets/#{@data.fetch(:current_market).fetch(:id)}/order_bids/clear", remote: true, method: :post, data: {confirm: t('.confirm_clear_bids')}, class: 'cancelButton' %>
        </li>
        <li>
          <%= link_to t('.clear_asks_html'), "/markets/#{@data.fetch(:current_market).fetch(:id)}/order_asks/clear", remote: true, method: :post, data: {confirm: t('.confirm_clear_asks')}, class: 'cancelButton' %>
        </li>
        <li>
          <%= link_to t('.clear_all_html'),  "/markets/#{@data.fetch(:current_market).fetch(:id)}/orders/clear", remote: true, method: :post, data: {confirm: t('.confirm_clear_all')}, class: 'cancelButton' %>
        </li>
      </ul>
    </div>
    <div class="panel-body panel-body-content">
      <div class="empty-row">
        <%= t('.no_open_orders') %>
      </div>
      <table class="table table-hover">
        <tbody></tbody>
      </table>
    </div>
    <%= render partial: 'mask' %>
  </div>
</div>
