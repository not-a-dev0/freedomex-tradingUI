<li id="settings" class="dropdown nav-item account-settings text-left active">
  <a class="dropdown-toggle" href="#" data-toggle="dropdown">
    <span class="user-icon"><img src="/assets/default-profile.png"></span><span class="email-limit"><%= @data.fetch(:current_member).fetch(:email) %></span>
  </a>
  <ul class="dropdown-menu">
    <li>
      <a>
        <input type="checkbox" name="sound-checkbox" data-size="mini" data-on-color="success">
      </a>
    </li>
    <li>
      <a>
        <input type="checkbox" name="notification-checkbox" data-size="mini" data-on-color="success">
      </a>
    </li>
    <li>
      <a href=<%= generate_ui_url('funds') %> target="_blank">
